		"delay": "^4.4.1",
		"husky": "^4.2.5",
		"nock": "^13.1.0",
		"tempfile": "^3.0.0",
		"typescript": "^4.3.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run test && npm run lint",
			"pre-push": "npm run test && npm run lint"
