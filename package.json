		"arvis-notifier": "^0.0.1",
		"arvis-store": "^0.2.10",
		"cache-conf": "^0.6.0",
		"chalk": "^4.1.1",
		"clean-stack": "^2.2.0",
		"conf": "^5.0.0",
		"dot-prop": "^5.1.0",
		"execa": "^2.1.0",
		"fast-glob": "^3.2.5",
